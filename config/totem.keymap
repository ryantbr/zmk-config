            // ┗━━━━━━━━━━━┻━━━━━━━━━━━┻━━━━━━━━━━━╋━━━━━━━━━━━╋━━━━━━━━━━━╋━━━━━━━━━━━┫   ┣━━━━━━━━━━━╋━━━━━━━━━━━╋━━━━━━━━━━━╋━━━━━━━━━━━┻━━━━━━━━━━━┻━━━━━━━━━━━┛
            //                                     ┃    DEL    ┃    TAB    ┃    SPC    ┃   ┃   ENTER   ┃           ┃   BSPC    ┃
            //                                     ┗━━━━━━━━━━━┻━━━━━━━━━━━┻━━━━━━━━━━━┛   ┗━━━━━━━━━━━┻━━━━━━━━━━━┻━━━━━━━━━━━┛

            bindings = <
                &kp Q        &kp W       &kp E           &kp R  &kp T        &kp Y    &kp U          &kp I            &kp O       &kp P
                &mt LCTRL A  &mt LALT S  &mt LEFT_GUI D  &kp F  &kp G        &kp H    &mt RSHFT J    &mt RIGHT_GUI K  &mt RALT L  &mt RCTRL SEMICOLON
&kp LEFT_SHIFT  &kp Z        &kp X       &kp C           &kp V  &kp B        &kp N    &kp M          &kp COMMA        &kp DOT     &kp FSLH             &kp SQT
                                         &mo 2           &mo 1  &kp SPACE    &kp RET  &kp BACKSPACE  &mo 2
